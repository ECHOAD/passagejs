    "@babel/plugin-proposal-optional-chaining": "7.21.0",
    "@babel/plugin-transform-runtime": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@cosmwasm/ts-codegen": "1.11.1",
    "@types/jest": "^29.5.14",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.7.0",
    "babel-watch": "^7.8.1",
